meta {
  name: update purchase status
  type: http
  seq: 1
}

patch {
  url: {{baseUrl}}/api/compras/{{purchaseId}}/status
  body: json
  auth: inherit
}

headers {
  Content-Type
  : application/json
}

body:json {
  {
    "email": "{{email}}",
    "password": "{{password}}",
    "callbackURL": "/",
    "status": "pending"
  }
  
  
}

settings {
  encodeUrl: true
  timeout: 0
}
