meta {
  name: update email
  type: http
  seq: 1
}

patch {
  url: {{baseUrl}}/api/users/{{userId}}/email
  body: json
  auth: inherit
  headers {
    Content-Type: application/json
    Authorization: Bearer {{token}}
  }
}

body:json {
  {
    "email": "palmeiras@gigante.com"
  }
}

tests {
  expect res.status toBe 200
  expect res.body.email toBe "palmeiras@gigante.com"
}
